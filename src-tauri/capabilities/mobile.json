{
  "$schema": "../gen/schemas/mobile-schema.json",
  "identifier": "mobile",
  "description": "Mobile capabilities for the main window",
  "platforms": [
    "android"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "opener:default",
    "opener:allow-default-urls",
    "core:default",
    "fs:read-all",
    "fs:write-all",
    "clipboard-manager:default",
    "clipboard-manager:allow-write-text"
  ]
}