    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "@tauri-apps/api": "2.10.1",
    "@tauri-apps/plugin-clipboard-manager": "2.3.2",
    "@tauri-apps/plugin-fs": "2.4.5",
    "@tauri-apps/plugin-opener": "~2.5.3",
    "@tauri-apps/plugin-os": "~2.3.2",
    "pretty-bytes": "^7.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router": "7.13.0",
    "react-router-dom": "7.13.0",
    "react-window": "^2.2.7",
    "zustand": "5.0.11"
