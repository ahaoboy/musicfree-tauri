{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "musicfree",
  "version": "0.1.2",
  "identifier": "com.ahaoboy.musicfree",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "musicfree",
        "width": 393,
        "height": 852,
        "decorations": true,
        "transparent": true,
        "resizable": false,
        "center": true
      }
    ],
    "security": {
      "csp": null,
      "freezePrototype": false,
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "**"
          ]
        }
      },
      "capabilities": ["desktop"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "passive"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEEzRDYwMzQ1MTlBRTVGRDIKUldUU1g2NFpSUVBXbzFhajl3bEc2bmt4VzR0bzZ3b2dLS2JrZVhxQnVWNnF4OEJ6VGkvRHBsQWEK",
      "endpoints": [
        "https://github.com/ahaoboy/musicfree-tauri/releases/latest/download/latest.json"
      ]
    }
  }
}